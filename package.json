{
  "name": "@cdc/lego-engine",
  "version": "1.0.1",
  "description": "A page engine, which render front end page with low-code.",
  "author": "Stanley",
  "homepage": "",
  "files": [
    "dist",
    "lib",
    "es"
  ],
  "sideEffects": [
    "*.css",
    "*.less"
  ],
  "scripts": {
    "build": "npm run clean && npm run build-lib && npm run build-es",
    "build-lib": " cross-env BUILD_TYPE=lib father-build",
    "build-es": "cross-env BUILD_TYPE=es father-build",
    "build-es:watch": "npm run clean &&  cross-env BUILD_TYPE=es father-build --watch",
    "css": "node scripts/generate-css.js",
    "webpack": "webpack",
    "clean": "rm -rf dist es lib",
    "postversion": "git push",
    "prepublishOnly": "npm run build"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "browserslist": [
    "last 2 versions",
    "Firefox ESR",
    "> 1%",
    "ie >= 11"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "@babel/runtime": "^7.18.3",
    "axios": "^0.21.1",
    "brace": "^0.11.1",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.2",
    "immutability-helper": "^3.1.1",
    "lodash": "^4.17.15",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "query-string": "^4.3.4",
    "react-ace": "^9.4.3",
    "react-color": "^2.19.3",
    "react-copy-to-clipboard": "^5.0.3",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-redux": "^7.2.1",
    "react-resizable": "^1.11.0",
    "redux": "^4.0.5",
    "short-uuid": "^4.1.0",
    "tiny-warning": "^1.0.3",
    "unstated-next": "^1.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.17.12",
    "antd": "~4.20.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-async-to-promises": "^0.8.18",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.1",
    "dotenv": "^8.2.0",
    "father-build": "^1.20.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "license": "MIT"
}
